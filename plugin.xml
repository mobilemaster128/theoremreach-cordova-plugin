<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-theoremreach" version="0.0.2" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>TheoremReachPlugin</name>
    <description>Cordova plugin for TheoremReach</description>
    <license>Apache 2.0</license>
    <keywords>cordova,theoremreach</keywords>
    <js-module name="TheoremReachPlugin" src="www/TheoremReachPlugin.js">
        <clobbers target="cordova.plugins.TheoremReachPlugin" />
    </js-module>
    <engines>
        <engine name="cordova" version=">=6.4.0" />
        <engine name="cordova-ios" version=">=4.3.0" />
        <engine name="cordova-android" version=">=5.0.0" />
        <engine name="apple-xcode" version=">=8.0.0" />
        <engine name="apple-ios" version=">=7.0.0" />
    </engines>
    <platform name="android">
        <framework src="com.google.android.gms:play-services-ads:9.0.1" />
        <framework src="com.theoremreach:theoremreach:3.0.0.7" />
        <framework src="build-extras-theoremreach.gradle" custom="true" type="gradleReference" />
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="TheoremReachPlugin">
                <param name="android-package" value="com.plugin.theoremreach.TheoremReachPlugin" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml">
            <uses-permission android:name="android.permission.INTERNET" />
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml" />
        <source-file src="src/android/TheoremReachPlugin.java" target-dir="src/com/plugin/theoremreachplugin/" />
    </platform>
    <!--platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="TheoremReachPlugin">
                <param name="ios-package" value="TheoremReachPlugin" />
            </feature>
        </config-file>
        <source-file src="src/ios/TheoremReachPlugin.m" />
    </platform-->
</plugin>