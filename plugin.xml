<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-theoremreach" version="0.0.1" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>TheoremReach</name>
    <js-module name="TheoremReach" src="www/TheoremReach.js">
        <clobbers target="cordova.plugins.TheoremReach" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="TheoremReach">
                <param name="android-package" value="cordova-plugin-theoremreach.TheoremReach" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml" />
        <source-file src="src/android/TheoremReach.java" target-dir="src/cordova-plugin-theoremreach/TheoremReach" />
    </platform>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="TheoremReach">
                <param name="ios-package" value="TheoremReach" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml">
            <uses-permission android:name="android.permission.INTERNET" />
        </config-file>
        <source-file src="src/ios/TheoremReach.m" />
    </platform>
</plugin>